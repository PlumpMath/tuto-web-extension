{
	"manifest_version": 2,
	"name": "Extension DWAPS",
	"version": "0.1.0",

	"description": "Cache le texte de la page web courante au moyen d'un bouton de contr√¥le.",

	"icons":
	{
		"16": "icons/logo-16x16.png",
		"32": "icons/logo-32x32.png",
		"48": "icons/logo-48x48.png",
		"96": "icons/logo-96x96.png",
		"512": "icons/logo-512x512.png"
	},

	"homepage_url": "http://dwaps.fr",

	"content_scripts":
	[
		{
			"matches": [ "http://*/*" ],
			"js": [ "js/page.js" ]
		}
	],

	"permissions":
	[
		"tabs"
	]
}